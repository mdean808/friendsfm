{
  "name": "friends-fm",
  "private": true,
  "version": "0.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "cap:build": "yarn build && npx cap sync",
    "ios:build": "yarn build && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:run": "yarn ios:build && npx cap run ios --target 0F14861A-995E-4B36-AA3F-14F1367CF132",
    "ios:run:morgan": "yarn build && npx cap run ios --target 00008120-000E49243AC0C01E",
    "android:build": "yarn build && npx cap sync android",
    "android:open": "npx cap open android",
    "android:run": "yarn android:build && npx cap run android --target Pixel_6_Pro_API_32",
    "firebase:deploy": "cd ./functions && firebase deploy",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
